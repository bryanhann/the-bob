source $BOB/tests/source/config.sh
info "BEGIN TESTING THE ENVIRONMENT"
info "found: \$TEMP is ${TEMP}"
info "found: \$TMP is ${TMP}"

runtest \
	"\$TMP is \$TEMP" \
	"[ .$TMP == .$TEMP  ]"\
	

runtest \
	"\$TMP is directory" \
	"testdir $TMP"\
	

runtest \
	"\$HOME/.local/bin is directory" \
	"testdir $HOME/.local/bin"\
	

info "BEGIN TESTING THE TESTING FRAMEWORK"

runtest\
	"test return_with()" \
	"return_with 42"\
	--err 42

